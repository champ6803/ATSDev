services:
  testapp:
    build:
      context: D:\Workspace\ATSDev\TestApp
      dockerfile: TestApp/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTPS_PORT: '44364'
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: testapp
    ports:
    - published: 64747
      target: 80
    - published: 44364
      target: 443
    volumes:
    - C:\Users\Admin\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Admin\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
version: '3.4'